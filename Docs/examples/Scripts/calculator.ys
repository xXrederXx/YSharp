PRINT("Number 1")
VAR x = INPUT().ToNumber()
PRINT("Operator")
VAR op = INPUT()
PRINT("Number 2")
VAR y = INPUT().ToNumber()

IF op == "+" THEN
    PRINT(x + y)
END
ELIF op == "-" THEN
    PRINT(x - y)
END
ELIF op == "*" THEN
    PRINT(x * y)
END
ELIF op == "/" THEN
    PRINT(x / y)
END
ELSE
    PRINT("Invalid Op")
END