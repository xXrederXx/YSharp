PRINT("Number 1")
VAR x = INPUT()
VAR x = x.ToNumber()
PRINT("Operator")
VAR op = INPUT()
PRINT("Number 2")
VAR y = INPUT()
VAR y = y.ToNumber()

IF op == "+" THEN
    PRINT(x + y)
END

IF op == "-" THEN
    PRINT(x - y)
END

IF op == "*" THEN
    PRINT(x * y)
END
IF op == "/" THEN
    PRINT(x / y)
END